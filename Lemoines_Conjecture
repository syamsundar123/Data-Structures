#Lemoine's  Conjecture
from math import sqrt


def isprime(n):
    if n<2:
        return False
    for i in range(2,int(sqrt(n))+1):
        if n%i == 0:
            return False
    return True


def lemoines_conjecture(n:int) -> None:

    for p in range(1,n//2 + 1):
        q = n - (2*p)

        if isprime(q) and isprime(p):
            print(n," = 2 * ",p," + ",q)


if __name__ == '__main__':
    n = int(input())
    lemoines_conjecture(n)

